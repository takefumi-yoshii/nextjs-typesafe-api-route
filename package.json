{
  "name": "nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"lib/**/*.{ts,tsx}\"",
    "postinstall": "npm run gen:apitype",
    "gen:apitype": "npm run gen:apitype:clean && npm run gen:apitype:build",
    "gen:apitype:build": "ts-node ./lib/src/index.ts",
    "gen:apitype:clean": "rimraf ./src/types/pages/api",
    "gen:apitype:watch": "nodemon"
  },
  "dependencies": {
    "fs-extra": "^10.0.0",
    "next": "12.0.1",
    "query-string": "^7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/react": "^17.0.37",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "prettier-plugin-organize-imports": "^2.3.4",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
